              "fullDescription": {"text": "Server-side request forgery allows attackers to make requests from the server to internal resources."},
              "defaultConfiguration": {"level": "error"},
              "properties": {"security-severity": "9.0", "tags": ["security", "ssrf", "owasp-a10"]}
            },

              "fullDescription": {"text": "User input is directly concatenated into SQL query without parameterization, allowing attackers to execute arbitrary SQL commands."},
              "defaultConfiguration": {"level": "error"},
              "properties": {"security-severity": "9.8", "tags": ["security", "sql-injection", "owasp-a03"]}
            },
            {
              "id": "AUTH001",
              "name": "HardcodedCredentials",
              "shortDescription": {"text": "Hardcoded credentials detected"},
              "fullDescription": {"text": "Credentials are hardcoded in source code, exposing sensitive authentication data."},
              "defaultConfiguration": {"level": "error"},
              "properties": {"security-severity": "8.5", "tags": ["security", "credentials", "owasp-a07"]}
            },
            {
              "id": "XSS001",
              "name": "CrossSiteScripting",
              "shortDescription": {"text": "XSS vulnerability detected"},
              "fullDescription": {"text": "User input is rendered without sanitization, allowing script injection."},
              "defaultConfiguration": {"level": "error"},
              "properties": {"security-severity": "7.5", "tags": ["security", "xss", "owasp-a03"]}
            },
            {
              "id": "CRYPTO001",
              "name": "WeakCryptography",
